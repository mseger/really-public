<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="edu.olin.dotcomclass.Feedback"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-permission android:name="android.permission.INTERNET" />
    <application android:label="Feedback" android:icon="@drawable/icon">
        <activity android:name="FeedbackActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FeedbackViewActivity"
                android:label="Feedback Viewer"
                />
        <activity android:name=".EditPreferences" android:label="Edit Preferences"/>
        <activity android:name=".NewPostActivity" android:label="New Post"/>
    </application>
</manifest> 
